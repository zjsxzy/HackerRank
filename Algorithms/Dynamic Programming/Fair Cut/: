#include <cstring>
#include <iostream>
#include <algorithm>
using namespace std;
#define PB push_back
#define MP make_pair
#define SZ(v) ((int)(v).size())
#define abs(x) ((x) > 0 ? (x) : -(x))
#define FOREACH(e,x) for(__typeof(x.begin()) e=x.begin();e!=x.end();++e)
typedef long long LL;

const int MAXN = 3005;
const ll INF = 1e13;
int n, k;
int a[MAXN];
LL dp[MAXN][MAXN];

void solve() {
    memset(dp, -INF, sizeof(dp))
    for (int i = 0; i < n; i++) {
        if (i == 0) {
            dp[i][0] = dp[i][1] = 0;
        } else {
        }
    }
}

int main() {
    cin >> n >> k;
    for (int i = 1; i <= n; i++) {
        cin >> a[i];
    }
    sort(a + 1, a + n + 1);
    solve();
    return 0;
}

